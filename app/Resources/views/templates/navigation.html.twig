{{ dump(app) }}
<div class="navigation">
        <a href="{{ path('homepage') }}"><img src="{{ asset('favicon.ico') }}"/></a>
        <div class="group-btn">
            {% if is_granted("IS_AUTHENTICATED_FULLY") %}
                <h4 class="welcome">Welcome {{ app.user.username }}!</h4>
                <a class="btn btn-success" href="{{ path('fos_user_profile_show') }}">Profile</a>
                <a class="btn btn-success" href="{{ path('fos_user_security_logout') }}">Logout</a>
            {% else %}
                <a class="btn btn-primary" href="{{ path('fos_user_security_login') }}">Login</a>
                <a class="btn btn-primary" href="{{ path('fos_user_registration_register') }}">Register</a>
            {% endif %}
        </div>
</div>